{
  "compilerOptions": 
  {
    "target": "es6",
    "module": "none",
    "sourceMap": false,
    "allowJs": true,
    "outFile": "../build/client.js",
    "alwaysStrict" : true,
    "noImplicitAny" : true,
    "noImplicitReturns": true,
    "noImplicitThis" : true,
    "noUnusedLocals" : false
  },
  "include": [
    "*/*",
    "main.ts"
  ]
}
